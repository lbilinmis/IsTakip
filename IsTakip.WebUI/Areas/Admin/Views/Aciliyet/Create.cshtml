@model ViewModelAciliyetAdd
@{
    ViewData["Title"] = "Create";
}

<section class="content-main">
    <div class="row">
        <div class="col-md-12">
            <div class="card mb-4">
                <div class="card-header">
                    <h4>Yeni Aciliyet Durumu</h4>
                </div>
                <div class="card-body">


                    <form method="post">

                        <div class="mb-4">
                            <label asp-for="Title" class="form-label"></label>
                            <span asp-validation-for="Title" class="text-danger"></span>
                            <input type="text" asp-for="Title" class="form-control" />
                        </div>

                        <button type="submit" class="btn btn-md rounded font-sm hover-up">Kaydet</button>
                    </form>
                </div>
            </div>
        </div>
    </div>

</section>

@section Scripts{

    <script src="../../root/backendTheme/assets/js/clientside/jqueryValidate/jquery.validate.min.js" type="text/javascript"></script>
    <script src="../../root/backendTheme/assets/js/clientside/queryUnob/jquery.validate.unobtrusive.min.js" type="text/javascript"></script>
}